package com.ecommerce.callingFunctions;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.testng.annotations.Test;

import com.ecommerce.pageObjects.PageObjects;

public class CallingFunctions extends PageObjects {
	
	public CallingFunctions(WebDriver driver) {
		super(driver);
	}

	private WebDriver driver;
	private String URL = "http://store.demoqa.com/";
	
	@Test
	public void OpenSite() {
		
		driver = new FirefoxDriver();
		driver.manage().window().maximize();
		driver.get(URL);
	}
	
	@Override
	

}

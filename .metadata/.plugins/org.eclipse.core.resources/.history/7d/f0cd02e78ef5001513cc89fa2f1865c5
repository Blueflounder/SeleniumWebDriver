package com.polymorphism.test;

import com.bitwise.test.MyFirstInterface;

public class ChildClass extends ParentClass implements MyFirstInterface {

	public void disp() {

		System.out.println("This is 1st child class method");
		
	}
	
	public void add (int i, int j){
		
		int total = i + j;
		System.out.println("Child Class: Addition is: "+total);
		
	}
	
	//Below are the interface methods
	public void showName(){
		
		System.out.println("This is interface method");
	}
	
	public String returnName(){
		
		String name = "Jai Ganesh";
		return name;
	}
	public int calculateSum(){
		
		int i=1;
		int j=2;
		int sum = i +j;
		return sum;
		
	}
}


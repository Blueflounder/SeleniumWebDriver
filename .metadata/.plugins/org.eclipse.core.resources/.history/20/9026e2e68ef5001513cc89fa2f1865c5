package com.inheritance.test;

public class FirstClass {
	
	int startGear;
	static int startSpeed;
	
	public FirstClass()//default constructor
	{
		System.out.println("This is default constructor of FirstClass");
		startGear++;
		startSpeed++;
		//System.out.println("Value of startGear is:"+" "+startGear);
		//System.out.println("Value of startSpeed is:"+" "+startSpeed);
	}
	
	private FirstClass(int sg, int sp)//overloaded constructor
	{
		System.out.println("This is overloaded constructor of FirstClass");
		startGear++;
		startSpeed++;
		System.out.println("Value of startGear is:"+" "+startGear);
		System.out.println("Value of startSpeed is:"+" "+startSpeed);
	}
	
	public int addition(int i, int j)
	{
		int add = i + j;
		System.out.println("This is public method of FirstClass and output of addition is:"+" "+add);
		return add;
	}
	
	protected int subtraction(int i, int j)
	{
		int sub = i - j;
		System.out.println("This is protected method of FirstClass and output of subtraction is:"+" "+sub);
		return sub;
	}
	
	private int multiplication(int i, int j)
	{
		int multi = i * j;
		System.out.println("This is private method of FirstClass and output of multiplication is:"+" "+multi);
		return multi;
	}
	
	int division(int i, int j)
	{
		int div = i / j;
		System.out.println("This is private method of FirstClass and output of division is:"+" "+div);
		return div;
		
	}

}
